
<html>
<head>
    <title>The Log Analyser</title>
    <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.css">
    <style>
        .space {
            margin: 10px;
        }
    </style>

</head>
<body>

<div class="container">
    <div class="jumbotron">
        <h1>Log Analyser</h1>
    </div>

    <div class="" id="fileReader">
        <div class="row panel" id="keywords">

        </div>

        <div class="form-group">
        <input class="form-control" type="text" placeholder="Enter search query" name="search" id="searchQuery" />
        </div>

        <div class="row">
        <div class="col-lg-3">
            <input class="btn btn-default" class="input-control" type="file" id="fileInput">
        </div>

        <div class="col-lg-3">
            <button class="btn btn-default" type="submit" name="button" id="next">Next</button>
        </div>

        <div class="col-lg-3" id="done">
            <button class="btn btn-primary" type="submit" name="button" id="done">Done Highlighting</button>
        </div>

        <div class="col-lg-3" id="reset">
            <button class="btn btn-primary" type="reset" name="button" id="reset">Reset</button>
        </div>

        </div>

        <div class="row">
            <div class="col-lg-4">
                <button class="btn btn-primary" id="timestamp-select">Select Timestamp</button>
            </div>
        </div>

        <div id="chart_div"></div>

    <div class="panel" id="fileDisplayArea">
    </div>

</div>
</div>



<script src="superagent.js"></script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script src="bower_components/handlebars/handlebars.js"></script>
<script src="bower_components/axios/dist/axios.js"></script>



<script src="js/gui/controller/Controller.js"></script>
<script src="js/gui/models/Model.js"></script>
<script src="js/gui/views/View.js"></script>

<script src="js/gui/templates/ChartDisplay.js"></script>
<script src="js/gui/templates/KeywordDisplay.js"></script>

<script src="js/gui/constants.js"></script>
<script src="js/gui/app.js"></script>
</body>
</html>


